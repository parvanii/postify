generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AIOutput {
  id           Int     @id @default(autoincrement())
  formData     String?   // varchar
  aiResponse   String    @db.Text
  templateSlug String?   // varchar
  createdBy    String?   // varchar
  createdAt    String?   // varchar 

  @@map("aiOutput") 
}

model UserSubscription {
  id        Int      @id @default(autoincrement())
  email     String?
  userName  String?
  active    Boolean?
  paymentId String?
  joinDate  String?

  @@map("userSubscription")
}
